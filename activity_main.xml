<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/lowcharge_entry"
        android:layout_width="106dp"
        android:layout_height="27dp"
        android:layout_marginTop="32dp"
        android:background="@null"
        android:hint="0.75"
        android:inputType="numberDecimal"
        android:maxLength="6"
        android:text="1.25"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/money8"
        app:layout_constraintHorizontal_bias="0.312"
        app:layout_constraintStart_toEndOf="@+id/rate_text"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <TextView
        android:id="@+id/lowcharge_text"
        android:layout_width="102dp"
        android:layout_height="27dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Min Charge ($)"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/money8"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toEndOf="@+id/money"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <TextView
        android:id="@+id/rate_text"
        android:layout_width="89dp"
        android:layout_height="29dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Rate ($/ft)"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/money8"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toEndOf="@+id/money"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/rate_entry"
        android:layout_width="106dp"
        android:layout_height="27dp"
        android:layout_marginTop="4dp"
        android:background="@null"
        android:hint="0.75"
        android:inputType="numberDecimal"
        android:maxLength="6"
        android:text="0.75"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/money8"
        app:layout_constraintHorizontal_bias="0.304"
        app:layout_constraintStart_toEndOf="@+id/rate_text"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <ImageView
        android:id="@+id/money12"
        android:layout_width="68dp"
        android:layout_height="29dp"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="18dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/get_estimate"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@raw/mulah" />

    <ImageView
        android:id="@+id/money13"
        android:layout_width="68dp"
        android:layout_height="29dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="18dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/get_estimate"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@raw/mulah" />

    <ImageView
        android:id="@+id/lam2"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="59dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/width_text"
        app:layout_constraintEnd_toStartOf="@+id/title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.009"
        app:srcCompat="@raw/lam2" />

    <ImageView
        android:id="@+id/lam"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="37dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/width_entry"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/title"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.009"
        app:srcCompat="@raw/lam" />

    <ImageView
        android:id="@+id/money8"
        android:layout_width="68dp"
        android:layout_height="29dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="25dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/estimated_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@raw/mulah" />

    <ImageView
        android:id="@+id/money"
        android:layout_width="68dp"
        android:layout_height="29dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="18dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/estimated_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@raw/mulah" />

    <TextView
        android:id="@+id/width_text"
        android:layout_width="143dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Paper Side Width (in)"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title"
        app:layout_constraintVertical_bias="0.035"
        app:layout_wrapBehaviorInParent="vertical_only" />

    <TextView
        android:id="@+id/perline_text"
        android:layout_width="143dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Num Per Line"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/width_text"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/numpages_entry"
        android:layout_width="160dp"
        android:layout_height="42dp"
        android:layout_marginStart="-55dp"
        android:background="@null"
        android:hint="15 pages total"
        android:inputType="number"
        android:maxLength="6"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toEndOf="@+id/perline_text"
        app:layout_constraintTop_toBottomOf="@+id/perline_entry"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/width_entry"
        android:layout_width="160dp"
        android:layout_height="42dp"
        android:layout_marginStart="-55dp"
        android:layout_marginTop="-15dp"
        android:background="@null"
        android:hint="8.5 in width"
        android:inputType="numberDecimal"
        android:maxLength="6"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toEndOf="@+id/perline_text"
        app:layout_constraintTop_toBottomOf="@+id/title"
        app:layout_constraintVertical_bias="0.038" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/perline_entry"
        android:layout_width="160dp"
        android:layout_height="42dp"
        android:layout_marginStart="-55dp"
        android:background="@null"
        android:hint="2 per line"
        android:inputType="number"
        android:maxLength="6"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toEndOf="@+id/perline_text"
        app:layout_constraintTop_toBottomOf="@+id/width_entry"
        app:layout_constraintVertical_bias="0.002" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/numaverage_entry"
        android:layout_width="160dp"
        android:layout_height="42dp"
        android:layout_marginStart="-55dp"
        android:background="@null"
        android:hint="2.7816 in average"
        android:inputType="numberDecimal"
        android:maxLength="6"
        android:singleLine="false"
        android:text="2.7816"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toEndOf="@+id/perline_text"
        app:layout_constraintTop_toBottomOf="@+id/numpages_entry"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/numpages_text"
        android:layout_width="143dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:linksClickable="false"
        android:text="Page Count"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/perline_text"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/average_text"
        android:layout_width="175dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:linksClickable="false"
        android:text="Space Between Pages (in)"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numpages_text"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/title"
        android:layout_width="195dp"
        android:layout_height="46dp"
        android:fontFamily="cursive"
        android:text=" Lam Estimate"
        android:textAllCaps="false"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="35dp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <Button
        android:id="@+id/get_estimate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#6B9F03"
        android:text="How much mulah???!!!"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title"
        app:layout_constraintVertical_bias="0.864" />

    <View
        android:id="@+id/divider"
        android:layout_width="409999dp"
        android:layout_height="10dp"
        android:layout_marginTop="50dp"
        android:background="@color/black"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numpages_entry"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/estimated_text"
        android:layout_width="390dp"
        android:layout_height="132dp"
        android:layout_marginTop="101dp"
        android:ems="10"
        android:text="$"
        android:textAlignment="center"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/get_estimate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/donate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:backgroundTint="#0D5308"
        android:text="Donate"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/help_about_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:backgroundTint="#0D5308"
        android:text="Help"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/github"
        app:layout_constraintStart_toEndOf="@+id/donate"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/github"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:backgroundTint="#0D5308"
        android:text="Github"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
